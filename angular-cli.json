{
  "project": {
    "version": "1.0.0-beta.24",
    "name": "firebase-crud"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "../node_modules/tinymce/tinymce.js",
        "../node_modules/tinymce/themes/modern/theme.js",
        "../node_modules/tinymce/plugins/link/plugin.js",
        "../node_modules/tinymce/plugins/paste/plugin.js",
        "../node_modules/tinymce/plugins/table/plugin.js",
        "../src/app/profile/js/jquery.js",
        "../src/app/profile/js/material.min.js",
        "../src/app/profile/js/custom.js",
        "../src/app/profile/js/bootstrap.js",
        "../src/app/profile/javascript/aos.min.js",
        "../src/app/profile/javascript/materialize.min.js",
        "../src/app/profile/javascript/retina.min.js",
        "../src/app/profile/javascript/jquery.filterizr.min.js",
        "../src/app/profile/javascript/swiper.jquery.min.js",
        "../src/app/profile/javascript/custom-script.js",
        "../src/app/friend/mui.js",
        "../src/app/chat2/index.js"
      ],
      "environmentSource": "environments/environment.ts",
"environments": {
"dev": "environments/environment.ts",
"prod": "environments/environment.prod.ts"
}
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
